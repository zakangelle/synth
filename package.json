{
  "name": "wavedef",
  "version": "0.1.2",
  "description": "A synthesizer library built on the Web Audio API",
  "repository": {
    "type": "git",
    "url": "https://github.com/zakangelle/wavedef.git"
  },
  "devEngines": {
    "node": "8.9.4"
  },
  "engines": {
    "node": "8.9.4"
  },
  "author": "Zak Angelle, Drake Champagne",
  "license": "MIT",
  "devDependencies": {
    "antd": "^3.5.1",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "^1.7.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.6",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "^22.4.2",
    "lodash.capitalize": "^4.2.1",
    "lodash.get": "^4.4.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react": "^16.3.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-prop-types": "^1.0.3",
    "resolve": "1.6.0",
    "serve": "^7.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "web-audio-test-api": "^0.5.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "dependencies": {
    "audio-context": "^1.0.3",
    "lodash.flatten": "^4.4.0",
    "mousetrap": "^1.6.1",
    "note-to-frequency": "^1.4.1",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "tunajs": "^1.0.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "start:prod": "cross-env NODE_ENV=production PORT=80 serve -s build",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "coverage": "npm test -- --coverage",
    "coverage:report": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "heroku-postbuild": "npm run start:prod"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/modules/**/*.{js,jsx,mjs}",
      "src/helpers/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/modules/**/?(*.)(spec|test).{js,jsx,mjs}",
      "<rootDir>/src/helpers/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "stage-0"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "func-names": 0,
      "no-new": 0,
      "no-plusplus": 0,
      "no-param-reassign": 0,
      "function-paren-newline": 0,
      "import/prefer-default-export": 0,
      "react/jsx-no-bind": 0,
      "react/jsx-filename-extension": 0,
      "react/prefer-stateless-function": 0,
      "import/no-extraneous-dependencies": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to",
            "hrefLeft",
            "hrefRight"
          ],
          "aspects": [
            "noHref",
            "invalidHref",
            "preferButton"
          ]
        }
      ]
    }
  }
}
